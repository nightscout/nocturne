{
  "name": "@nocturne/remote-function-codegen",
  "version": "1.0.0",
  "type": "module",
  "description": "Generates SvelteKit remote functions from OpenAPI spec",
  "main": "dist/index.js",
  "bin": {
    "generate-remote": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "generate": "node --import tsx src/index.ts",
    "dev": "node --import tsx src/index.ts --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3",
    "vitest": "^3.0.0"
  }
}
