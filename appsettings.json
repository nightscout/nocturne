{
  "UseRemoteDatabase": false,
  "ConnectionStrings": {
    "postgres": "Host=localhost;Port=5432;Database=nocturne;Username=nocturne_user;Password=nocturne_password"
  },
  "PostgreSql": {
    "ConnectionString": "Host=localhost;Port=5432;Database=nocturne;Username=nocturne_user;Password=nocturne_password",
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "MaxRetryCount": 3,
    "MaxRetryDelaySeconds": 30,
    "CommandTimeoutSeconds": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "NightscoutSecretKeyForJWTGenerationShouldBeAtLeast32Characters",
    "Issuer": "Nocturne",
    "Audience": "NightscoutClient",
    "ExpirationHours": 24
  },
  "ReverseProxy": {
    "Routes": {
      "nightscout": {
        "ClusterId": "nightscout",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "nightscout": {
        "Destinations": {
          "target": {
            "Address": "https://your-nightscout-instance.example.com/"
          }
        }
      }
    }
  },
  "DemoMode": {
    "Enabled": false,
    "IntervalMinutes": 1,
    "InitialGlucose": 120,
    "WalkVariance": 10,
    "MinGlucose": 70,
    "MaxGlucose": 250,
    "Device": "demo-cgm"
  },
  "Analytics": {
    "Enabled": false,
    "CollectionEndpoint": "https://analytics.nocturne.app/collect",
    "BatchIntervalMinutes": 60,
    "MaxBatchSize": 100,
    "IncludeSystemMetrics": true,
    "IncludeUsageMetrics": true,
    "IncludePerformanceMetrics": true,
    "RequestTimeoutSeconds": 30,
    "VerboseLogging": false
  },
  "CompatibilityProxy": {
    "NightscoutUrl": "",
    "NocturneUrl": "",
    "TimeoutSeconds": 30,
    "RetryAttempts": 3,
    "DefaultStrategy": "Nightscout",
    "EnableDetailedLogging": false
  },
  "DeviceHealth": {
    "HealthCheckIntervalMinutes": 15,
    "DataGapWarningMinutes": 30,
    "BatteryWarningThreshold": 20,
    "SensorExpirationWarningHours": 24,
    "CalibrationReminderHours": 12,
    "MaintenanceAlertCooldownHours": 4,
    "EnablePredictiveAlerts": true,
    "EnablePerformanceAnalytics": true,
    "MaxDevicesPerUser": 10,
    "DeviceRegistrationTimeoutSeconds": 30,
    "EnableDebugLogging": false
  },
  "Connectors": {
    "Dexcom": {
      "SyncIntervalMinutes": 0,
      "DexcomUsername": "",
      "DexcomPassword": "",
      "DexcomRegion": "us",
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    },
    "Glooko": {
      "SyncIntervalMinutes": 0,
      "GlookoEmail": "",
      "GlookoPassword": "",
      "GlookoTimezoneOffset": -8,
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    },
    "LibreLinkUp": {
      "SyncIntervalMinutes": 0,
      "LibreUsername": "",
      "LibrePassword": "",
      "LibreRegion": "US",
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    },
    "CareLink": {
      "SyncIntervalMinutes": 0,
      "CarelinkUsername": "",
      "CarelinkPassword": "",
      "CarelinkRegion": "us",
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    },
    "Nightscout": {
      "SyncIntervalMinutes": 0,
      "SourceEndpoint": "",
      "SourceApiSecret": "",
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    },
    "MyFitnessPal": {
      "SyncIntervalMinutes": 0,
      "SyncDays": 7,
      "MyFitnessPalUsername": "",
      "MyFitnessPalPassword": "",
      "NightscoutUrl": "",
      "NightscoutApiSecret": ""
    }
  }
}
